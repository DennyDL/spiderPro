var houseTitleMap ={
    "":"<a href='http://bj.house.qq.com/' bossZone='bjhousetab' target='_blank'></a> <span class='fn'>/</span> <a href='http://house.qq.com/mobile/' bossZone='bjhousetab' 
++++++++++++++1+++++++++++++++++
++++++++++++++2+++++++++++++++++
++++++++++++++3+++++++++++++++++
++++++++++++++4+++++++++++++++++
seData',//jsonpص
			success: function(data){
				//today = data[1].today;
				today = t? t : data[1].today;
				var gameData = data[1].data;
				for(var i in gameData) {
					if(gameData[i].date == today) {
						getList();
					}
				}
			},
			error: function(data){

			}
		});
		function getList() {
				$.ajax({
					url: api.list,
					dataType: 'jsonp',
					data : {
						columnId : 100000,
					   startTime : today,
						 endTime : today,
							from : 'qshou_nba',
							  _t : new Date().getTime()

					},
					jsonp: 'callback',
					jsonpCallback:'responseData',//jsonpص
					success: function(data){
						list = data.data[today];
						var len = list.length;
						var gameLen = 0;

						if(len > 0) {
							var sHtml = '';
							for(var i = 0; i < len; i ++ ) {
								var time = Format(list[i].startTime);
								// ǰʱ δ ʱ
								if(curTime < time) {
									gameLen ++;
									var startTimeLen= list[i].startTime.length;
									sHtml += ''+
									'<li>'+
										'<a target="_blank" href="http://sports.qq.com/kbsweb/game.htm?mid='+ list[i].mid +'">'+
										'<span class="nbaRed"> '+ list[i].startTime.substr((startTimeLen-8), 5) +' </span>'+
											list[i].leftName + 
										'<span class="nbaRed"> VS </span> '+
											list[i].rightName +
										' </a>'+
									'</li>';
								}
							}

							if(gameLen > 1) {
								//ೡ
								sHtml = sHtml + sHtml;
								maxUlWidth = gameLen * 206;
								$('#nbaGameUl').css('width', maxUlWidth * 2  + 'px');
								$('#nbaGameUl').html(sHtml);
								$('#nbaSaicheng').show();
								$('#nbaSaichengContent').scrollLeft(maxUlWidth);
								
								bindEvt();
							}else if(gameLen == 1){
								// һ ޻
								$('#nbaSaicheng .nbaBtnLeft').hide();
								$('#nbaSaicheng .nbaBtnRight').hide();
								$('#nbaGameUl').html(sHtml);
								$('#nbaSaicheng').show();
							}else{
								// ޱ չʾ
							}
						}
					},
					error: function(data){

					}
				});
		
		}
		// "2015-10-12 07:00:00" ==> 10/12/2015 07:00:00 => 
		function Format(str) {
			var arr = str.split(' ');
			var arrYMD = arr[0].split('-')
			var newTime = arrYMD[1] + '/'+arrYMD[2]+'/'+arrYMD[0] + ' ' + arr[1]
			var time = new Date(newTime).getTime();
			return time;
		}
		function bindEvt() {
			var st;
			var ul = $('#nbaGameUl');
			var div = $('#nbaSaichengContent');
			$('#sportsNba').delegate('.nbaBtnLeft', 'click', function() {
				if(div.is(":animated")) {
					return;
				}
				if(st) {
					clearTimeout(st);
				}
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl - 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == 0) {
							div.scrollLeft(maxUlWidth)
						}
						st = setTimeout(slide,5000);
					}
				);
			});
			$('#sportsNba').delegate('.nbaBtnRight', 'click', function() {
				if(div.is(":animated")) {
					return;
				}
				if(st) {
					clearTimeout(st);
				}
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl + 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == (2*maxUlWidth - 206)) {
							div.scrollLeft(maxUlWidth -206)
						}
						st = setTimeout(slide,5000);
					}
				);
			}); 

			
			function slide() {
				var sl = div.scrollLeft();
				div.animate(
					{scrollLeft : (sl + 206)},
					'normal',
					'swing',
					function () {
						if(div.scrollLeft() == (2*maxUlWidth - 206)) {
							div.scrollLeft(maxUlWidth -206)
						}
						
						st = setTimeout(slide,5000);
					}
				);
			}
			st = setTimeout(slide,5000);
		}
	}());


</script>
