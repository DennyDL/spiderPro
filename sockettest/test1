function Cn2PinYin(l1) {
    var l2 = l1.length;
    var I1 = "";
    var reg = new RegExp('[a-zA-Z0-9\- ]');
    for (var i=0;i<l2;i++) {
        var val = l1.substr(i,1);
        var name = arraySearch(val,PinYin);
        if(reg.test(val)) {
            I1 += val;
        } else if (name!==false) {
            I1 += name;
        }
    }
    I1 = I1.replace(/ /g,'-');
    while (I1.indexOf('--')>0) {
        I1 = I1.replace('--','-');
    }
    return I1;
}

function arraySearch(l1,l2){
    for (var name in PinYin){
        if (PinYin[name].indexOf(l1)!=-1) {
            return name; break;
        }
    }
    return false;
}

function get_cur_pinyin(content)
{
    var fn = Cn2PinYin;
    var arr = [];
    var ch;
    for(var i=0,n=content.length; i<n; i++)
    {
        ch = content.charAt(i);
        arr[i] = (ch<"һ" || ch>"")?ch:fn(ch);
    }
    return arr.join("");
}
