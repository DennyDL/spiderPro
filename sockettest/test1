    autocheck:function(proName,cityName){
        autoUrl = "";
        if(autoMap[proName]){
            autoUrl = autoMap[proName];
        }
        if(autoUrl != ""){
            var ajaxAuto = new Ajax({
                headers:{"If-Modified-Since":0},
                url:autoUrl,
                parameters:{"random":Math.random()},
                method:"get"
            });
            ajaxAuto.onComplete = function(responseObject){
                $("#autonews").children().eq(5).html(responseObject.textString);
            };
            ajaxAuto.start();
        }
    },
    autoPriceCheck:function(cityName){
      	$("#autoLowPriceUl .name a").each(function(){
          	var content = $(this).html().replace(/&quot;|\"/gi,"")
			content = unescape(content.replace(/\\(u[0-9a-zA-Z]{4})/g, "%$1"));
			$(this).html(content)
		});
		window.autoLowPrice = function(data){
			if(data.code == 0){
				for(var i = 0, len = data.data.length; i < len; i ++){
					if(data.data[i].cityid == autoPriceMap[cityName]){
						var autoPriceArr = [];
						for(var n = 0; n < 3; n++){
							autoPriceArr.push('<li bosszone="'+autoPriceBossMap[cityName]+'"><span class="name"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_name+'</a></span><span class="p1"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_amount+'</a></span><span class="p2"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_price+'</a></span><span class="p3"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">ʣ'+data.data[i].serial_info[n].sserial_enddays+'</a></span></li>');
						}
						$("#autoLowPriceUl").html(autoPriceArr.join(""));
						break;
					}
				}
			}
		}
		window.autoLowPriceDefault = function(data){
			if(data.code == 0){
				for(var i = 0, len = data.data.length; i < len; i ++){
					if(data.data[i].cityid == 0){
						var autoPriceArr = [];
						for(var n = 0; n < 3; n++){
							autoPriceArr.push('<li bosszone="'+autoPriceBossMap[cityName]+'"><span class="name"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_name+'</a></span><span class="p1"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_amount+'</a></span><span class="p2"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">'+data.data[i].serial_info[n].sserial_price+'</a></span><span class="p3"><a target="_blank" href="'+data.data[i].serial_info[n].sserial_url+'">ʣ'+data.data[i].serial_info[n].sserial_enddays+'</a></span></li>');
						}
						$("#autoLowPriceUl").html(autoPriceArr.join(""));
						break;
					}
				}
			}
		}
        if(autoPriceMap[cityName]){
        	QQ.LoadScript("http://www.qq.com/c/lowprice.js?callback=autoLowPrice&random="+Math.random(),function(){});
		}else{
        	QQ.LoadScript("http://www.qq.com/c/lowprice.js?callback=autoLowPriceDefault&random="+Math.random(),function(){});
		}
