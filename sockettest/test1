                    }
                    if(obj.data[i].ext.style.icon == "1"){ //΢ͼ
                        entRightDataArr.push('qm_i_1');
                    }else if(obj.data[i].ext.style.icon == "2"){ //Ƶͼ
                        entRightDataArr.push('qm_i_2');
