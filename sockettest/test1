            for(var i=10; i<18; i++){
                if(tubd_qhome_yw.gxh[i].is_locked && tubd_qhome_yw.gxh[i].is_locked == "2"){
                    tubdYwArr.push('<li>');
                    for(var n=0; n<tubd_qhome_yw.gxh[i].items.length; n++){
                        tubdYwArr.push('<a  class="');
                        if(tubd_qhome_yw.gxh[i].items[n].ext){
                            if(tubd_qhome_yw.gxh[i].items[n].ext.style.bold == "1"){ //Ӵ
                                tubdYwArr.push('Q-bold ');
                            }
                            if(tubd_qhome_yw.gxh[i].items[n].ext.style.color == "1"){ //ɫ
                                tubdYwArr.push('qm_c_1 ');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.color == "2"){ //ɫ
                                tubdYwArr.push('qm_c_2 ');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.color == "3"){ //ɫ
                                tubdYwArr.push('qm_c_3 ');
                            }
                            if(tubd_qhome_yw.gxh[i].items[n].ext.style.icon == "1"){ //΢ͼ
                                tubdYwArr.push('qm_i_1');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.icon == "2"){ //Ƶͼ
                                tubdYwArr.push('qm_i_2');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.icon == "3"){ //ͼƬͼ
                                tubdYwArr.push('qm_i_3');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.icon == "4"){ //ֱͼ
                                tubdYwArr.push('qm_i_4');
                            }else if(tubd_qhome_yw.gxh[i].items[n].ext.style.icon == "5"){ //ͼ
