    cancel_event:function(evt)
    {
        evt=evt||window.event;
        if(evt.preventDefault){
            evt.stopPropagation();
            evt.preventDefault();
        }else{
            evt.returnValue=false;
            evt.cancelBubble=true;
        }
    },
    click_keyword:function(obj,evt)
