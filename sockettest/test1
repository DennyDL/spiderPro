		var s = $(this).prop('outerHTML');
		QQ.localData.set("otherItem", s);     	
		divParent.children(":first").replaceWith(s);  
		$(this).parent().parent().html(listEle);
		rmLi();
		divParent[0].className = "navMoreActived";
