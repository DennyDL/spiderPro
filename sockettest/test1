                        }else if(obj.data[i].items[n].ext.style.icon == "5"){ //ͼ
                            entRightDataArr.push('audioico');
                        }
                    }
                    entRightDataArr.push('" target="_blank" href="'+obj.data[i].items[n].url+'">'+obj.data[i].items[n].title+'</a>');
                    if(obj.data[i].items[n].ext){ //
