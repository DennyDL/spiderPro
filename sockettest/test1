//ƽ̨
function platformCheck(){
    if(navigator.platform == "Mac68K" || navigator.platform == "MacPPC" || navigator.platform == "Macintosh" || navigator.platform == "MacIntel"){ //macϵͳ
        $("#select").addClass("macSelect");
    }
}
//
function fontCheck(){
    if(navigator.platform == "Win32" || navigator.platform == "Windows"){ //windowsϵͳ
        if($("#guess").width() != 86){ //û΢ź
            $("body").addClass("noYahei");
        }
    }
    if(navigator.platform == "Mac68K" || navigator.platform == "MacPPC" || navigator.platform == "Macintosh" || navigator.platform == "MacIntel"){ //macϵͳ
        $("body").addClass("mac");
    }
}
//л
function fontTurn(){
    if(window.location.toString().indexOf('type=song') != -1){ //л
        $("body").addClass("song");
        $("#diaocha").find("a").attr("href","http://exp.qq.com/ur/?urid=12565");
    }
}
/*ض*/
function toTopHide(){
    if(document.documentElement.scrollTop+document.body.scrollTop > 400){
        if(!(/AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent)))){
            document.getElementById("fankui").style.display = "block";
            document.getElementById("homeSet").style.display = "block";
            document.getElementById("toTop").style.display = "block";
          	document.getElementById("minidown").style.display =
++++++++++++++1+++++++++++++++++
++++++++++++++2+++++++++++++++++
++++++++++++++3+++++++++++++++++
++++++++++++++4+++++++++++++++++
 "block";
        }
    }else{
        document.getElementById("fankui").style.display = "none";
        document.getElementById("homeSet").style.display = "none";
        document.getElementById("toTop").style.display = "none";
      	document.getElementById("minidown").style.display = "none";
    }
}
/*ƶ豸ѡɸ*/
function stockMobile(){
    if(/Android|Windows Phone|webOS|iPhone|iPod|BlackBerry|iPad/i.test(navigator.userAgent)){
        $("#top-smartbox-du-sx-result").hide();
        $("#stocksearch").hide();
    }
}
